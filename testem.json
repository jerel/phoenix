{
  "framework": "mocha",
  "test_page": "web/test/testem/index.mustache",
  "before_tests": "node ./web/test/testem/combine.js",
  "serve_files": [
    "./web/test/testem/_build/**/*.es5.js"
  ],
  "src_files": [
    "./web/test/**/*_test.js",
    "./web/static/js/**/*.js"
  ],
  "parallel": 1,
  "launch_in_ci": [
    "SL_safari_public_current",
    "SL_safari_public_6_0",
    "SL_edge_public_current",
    "SL_internet_explorer_public_current",
    "SL_internet_explorer_public_10",
    "SL_internet_explorer_public_9",
    "SL_internet_explorer_public_8",
    "SL_firefox_public_current",
    "SL_firefox_public_24",
    "SL_chrome_public_current",
    "SL_chrome_public_26"
  ],
  "launch_in_dev": [
    "Chrome"
  ],
  "launchers": {
    "SL_safari_public_current": {
      "exe": "node_modules/.bin/saucie",
      "args": [
        "--tunnelDomains",
        "phoenix-server.dev",
        "--vmVersion",
        "dev-varnish",
        "-b",
        "safari",
        "-v",
        "9.0"
      ],
      "protocol": "tap"
    },
    "SL_safari_public_6_0": {
